<h1>Listing company_details</h1>

<table>
  <thead>
    <tr>
      <th>Company name</th>
      <th>Company type</th>
      <th>Pan</th>
      <th>Vat</th>
      <th>Registration number</th>
      <th>Company address</th>
      <th>Billing address</th>
      <th>Shipping address</th>
      <th>Company info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_details.each do |company_detail| %>
      <tr>
        <td><%= company_detail.company_name %></td>
        <td><%= company_detail.company_type %></td>
        <td><%= company_detail.pan %></td>
        <td><%= company_detail.vat %></td>
        <td><%= company_detail.registration_number %></td>
        <td><%= company_detail.company_address %></td>
        <td><%= company_detail.billing_address %></td>
        <td><%= company_detail.shipping_address %></td>
        <td><%= company_detail.company_info %></td>
        <td><%= link_to 'Show', company_detail %></td>
        <td><%= link_to 'Edit', edit_company_detail_path(company_detail) %></td>
        <td><%= link_to 'Destroy', company_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company detail', new_company_detail_path %>
